{
  "name": "hangs.html",
  "version": "0.0.1",
  "description": "hangs.html",
  "main": "src/content/index.js",
  "scripts": {
    "build": "npm run build:quiet -- --progress",
    "build:quiet": "npm run build:clean && set NODE_ENV=development&& webpack",
    "build:clean": "rimraf dist && mkdirp dist && cp res/.htaccess dist/ && cp res/zee-worker.js dist/",
    "build-prod": "npm run build:clean && set NODE_ENV=production&& webpack -p --progress",
    "build-prod-readable": "npm run build:clean && set NODE_ENV=production&& webpack --progress",
    "build-docs": "documentation build src/common/types/profile.js -f md > docs/processed-profile-format.md; cat docs/processed-profile-format.md",
    "eslint": "eslint index.js src",
    "eslint-fix": "eslint --fix index.js src",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -i 'src/content/reducers/**/*.js' -t html -t text",
    "flow-generate-libdefs": "flow-typed install",
    "publish": "rimraf public_html && cp -r dist public_html",
    "serve-static": "ws -d dist/ -s index.html -p 4216",
    "start": "mkdirp dist && cp res/zee-worker.js dist/ && set NODE_ENV=development&& node server.js",
    "start-no-hot": "npm run start",
    "start-prod": "npm run build-prod && npm run serve-static",
    "start-prod-readable": "npm run build-prod-readable && npm run serve-static",
    "test": "npm run build && npm run test-all",
    "test:quiet": "npm run build:quiet && npm run test-all",
    "test-all": "set NODE_ENV=test&& nyc mocha --compilers js:babel-core/register 'src/test/**/*.js'",
    "test-file": "set NODE_ENV=test&& mocha --compilers js:babel-core/register",
    "test-report": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "author": "Doug Thayer <dothayer@mozilla.com>",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/squarewave/hangs.html"
  },
  "devDependencies": {
    "array-move": "^1.0.0",
    "array-range": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bisection": "0.0.3",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "clamp": "^1.0.1",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.5",
    "css-loader": "^0.26.0",
    "eslint": "^3.10.2",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.4.0",
    "express": "^4.13.4",
    "fake-indexeddb": "^1.0.12",
    "fetch-jsonp": "^1.0.2",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.40.0",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.4.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "local-web-server": "^1.2.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.1",
    "nyc": "^10.0.0",
    "offline-plugin": "^4.5.2",
    "pretty-bytes": "^4.0.2",
    "query-string": "^4.2.3",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-perf": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-contextmenu": "^2.3.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1",
    "rimraf": "^2.5.4",
    "shallowequal": "^0.2.2",
    "style-loader": "^0.13.1",
    "text-encoding": "^0.6.4",
    "url": "^0.11.0",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-hot-middleware": "^2.13.2",
    "webworker": "0.8.4",
    "workerjs": "^0.1.1"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current",
            "browsers": "defaults"
          }
        }
      ],
      "flow"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "dependencies": {
    "chart.js": "^2.7.1",
    "photon-colors": "^1.3.1",
    "react-chartjs-2": "^2.6.4",
    "react-modal": "^2.3.2"
  }
}
